#pojemnik {
  width: 50px;
  height: 50px;
  background: red;
  position: relative;
  animation: anim1 5s infinite;
}

@keyframes anim1 {
  from   {top: 0px; left: 0px;}
  50%  {top: 300px; left: 500px; background: black;}
  75%  {top: 100px; left: 0px; background: green;}
  to {top: 0px; left: 0px;}
}